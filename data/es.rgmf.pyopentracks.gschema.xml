<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="pyopentracks">
  <schema id="es.rgmf.pyopentracks" path="/es/rgmf/pyopentracks/" gettext-domain="es.rgmf.pyopentracks">
    <key name="dbversion" type="i">
      <default>0</default>
      <summary>Database version</summary>
      <description>Database version actually used.</description>
    </key>
    <key name="activitiespath" type="s">
      <default>''</default>
      <summary>Activity's Path</summary>
      <description>Select a directory where PyOpenTracks will load all activities from.</description>
    </key>
    <key name="last-folder" type="s">
      <default>''</default>
      <summary>Last Open Folder Path</summary>
      <description>Last open or visited folder to import, save, open...</description>
    </key>
    <key name="heart-rate-max" type="i">
      <default>-1</default>
      <summary>Training heart rate maximum</summary>
      <description>PyOpenTracks can use maximum heart rate to show heart rate zones.</description>
    </key>
    <key name="heart-rate-zones" type="s">
      <default>'50,60,70,80,90,100'</default>
      <summary>Training heart rate maximum</summary>
      <description>PyOpenTracks can use maximum heart rate to show heart rate zones.</description>
    </key>
    <key name="win-state-width" type="i">
      <default>800</default>
      <summary>Window's width</summary>
      <description>The window's width state.</description>
    </key>
    <key name="win-state-height" type="i">
      <default>600</default>
      <summary>Window's height</summary>
      <description>The window's height state.</description>
    </key>
    <key name="win-state-x" type="i">
      <default>0</default>
      <summary>Window's x position</summary>
      <description>The window's x position.</description>
    </key>
    <key name="win-state-y" type="i">
      <default>0</default>
      <summary>Window's y position</summary>
      <description>The window's y position.</description>
    </key>
    <key name="win-state-is-mamixmized" type="b">
      <default>false</default>
      <summary>Window's maximized</summary>
      <description>The window's maximized state.</description>
    </key>
  </schema>
</schemalist>
